#include cairoGraphicsLib.tc
#include pps/library.tc

main[
    draw_setup  //required SETUP
    testCaigra
    pl""
    code_peek
]

testCaigra [
    ps"demo of available draw codes"
    start "test_draw", 600, 600  //required START
    rectangle 150,30,240,50
    moveto  100, 100
    lineto  500, 500
    next
    ellipse 200,250,200,100
    next
    arc     300, 300, 150, 90, 180
    next
    arc     450, 150, 30, 0, 360
    dot 100, 120
    show
]
