#include pyGraphicsLib.tc
#include pps/library.tc

main[
    testCaigra
    pl""
]

testCaigra [
    start "test_draw", 600, 600
    rectangle 150,30,240,50
    moveto  100, 100
    lineto  500, 500
    next
    ellipse 200,250,200,100
    next
    arc     300, 300, 150, 90, 180
    next
    arcneg     300, 300, 150, 0, 270
    next
    arc     450, 150, 30, 0, 360
    dot 100, 120
    moveto 100, 150
    setfontsize 20
    showtext "see the dot"
    show
]
